# Strategic Integration Service Environment Variables
# Copy this to .env and configure for your environment

# Jira Configuration
JIRA_API_TOKEN=your_jira_api_token_here
JIRA_EMAIL=***REMOVED***
JIRA_BASE_URL=https://***REMOVED***

# Application Configuration
DEBUG=true
LOG_LEVEL=INFO

# UI Foundation Configuration
L2_DIVISION_FILTER="UI Foundations"
L2_CUSTOM_FIELD_PRIORITY="cf[18272]"

# Output Directories
OUTPUT_BASE_DIR=workspace/current-initiatives
REPORT_OUTPUT_DIR=executive/reports

# Security Settings
MASK_PII=true
ENCRYPT_SENSITIVE_DATA=true
AUDIT_LOG_ENABLED=true

# Performance Settings
ENABLE_CACHING=true
CACHE_TTL_SECONDS=300
PARALLEL_REQUESTS=5
